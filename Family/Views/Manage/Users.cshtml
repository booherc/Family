@{
    ViewBag.Title = "Users";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Users</h2>
<table>
    <thead>
        <tr>
            <th data-field="Full Name">Full Name</th>
            <th data-field="name">Email</th>
            <th data-field="price">Phone</th>
        </tr>
    </thead>
    @foreach (var user in Model)
    {
        <p>@user.UserName</p>


    }
</table>